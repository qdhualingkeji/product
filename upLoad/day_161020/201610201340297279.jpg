	        function getCommon_siteNameForCode(code){
		        if(!code || code=="china") return "全国";
		        else if(code=="ningbo") return "宁波";
		        else if(code=="hengyang") return "衡阳";
		        else if(code=="hainan") return "海南";
		        else return null;
		    }
	        function getCommon_SiteName1(){
	        	var top_stie_name=getCookie("YZZ_INDEX_VISTS_SITE_NAME");
	        	var top_stie_ocde=getCookie("YZZ_CURR_VISTS_SITE_CODE");
	        	var is_All="" || false;
	        	if(is_All){
		        	top_stie_name="全国";
		        	top_stie_ocde="china";
	        	}else{
		        	var isCity="";
		        	var isCiteName="";
		        	if(isCiteName!=""){
			        	top_stie_ocde=isCity;
			        	 top_stie_name=isCiteName;
		        	}else{
		        		if(!isCity || isCity=="china"){
				        	top_stie_name="全国";
				        	top_stie_ocde="china";
		        		}else{
			        		top_stie_ocde=isCity;
			        		top_stie_name=getCommon_siteNameForCode(top_stie_ocde);
			        		if(!top_stie_name){
				        		$.ajax({type:"POST",url: "Group_findSitesName",data: "site="+isCity,async: false,
									success: function(msg){top_stie_name=msg;}
			    				});
			        		}
				        }
		        	}
	        	}
	        	setCookie("YZZ_INDEX_VISTS_SITE_NAME",top_stie_name,30);
	        	setCookie("YZZ_CURR_VISTS_SITE_CODE",top_stie_ocde,30);
	        	return top_stie_name;
	        }
        	//document.write(getCommon_SiteName1());
        </script>
